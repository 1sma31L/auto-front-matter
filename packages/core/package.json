{
	"name": "auto-front-matter",
	"type": "module",
	"version": "0.0.4",
	"description": "Automatic Front Matter for Markdown",
	"author": "CondorHero",
	"license": "MIT",
	"homepage": "https://github.com/condorheroblog/auto-front-matter#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/condorheroblog/auto-front-matter.git"
	},
	"bugs": {
		"url": "https://github.com/condorheroblog/auto-front-matter/issues"
	},
	"keywords": [
		"template-front-matter",
		"auto-front-matter",
		"last-modified",
		"front-matter",
		"count-read",
		"duration"
	],
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.js",
			"import": "./dist/esm/index.js"
		}
	},
	"main": "dist/index.js",
	"module": "./dist/esm/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"bin": {
		"auto-front-matter": "./bin/auto-front-matter.mjs"
	},
	"files": [
		"dist",
		"bin"
	],
	"scripts": {
		"prepublishOnly": "nr build",
		"dev": "esno ./src/index.ts",
		"coverage": "vitest run --coverage",
		"test": "vitest",
		"build": "tsup",
		"build:watch": "tsup --watch"
	},
	"dependencies": {
		"cac": "^6.7.14",
		"chokidar": "^3.5.3",
		"fast-glob": "^3.2.12",
		"gray-matter": "^4.0.3",
		"inquirer": "^9.2.0",
		"reading-time": "^1.5.0",
		"simple-git": "^3.18.0"
	},
	"devDependencies": {
		"@types/inquirer": "^9.0.3"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
